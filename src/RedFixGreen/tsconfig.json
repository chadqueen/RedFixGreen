{
  "compilerOptions": {
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noEmitHelpers": false
  },
  "exclude": [
    "node_modules",
    "wwwroot",
    "typings/index.d.ts",
    "typings/globals/node",
    "typings/globals/jasmine",
    "typings/globals/require",
    "typings/globals/angular-protractor",
    "typings/globals/selenium-webdriver"
  ],
  "compileOnSave": false,
  "buildOnSave": false
}